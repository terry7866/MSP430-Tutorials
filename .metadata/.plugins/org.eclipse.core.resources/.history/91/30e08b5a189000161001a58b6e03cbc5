#include <msp430g2553.h>

/*
 * main.c
 */
int main(void) {
    WDTCTL = WDTPW | WDTHOLD;	// Stop watchdog timer

	if (CALBC1_1MHZ == 0xFF || CALDC0_1MHZ)
	{
		while(1);
	}

BCSCTL1 = CALBC1_1MHZ;
DCOCTL = CALDCO_1MHZ;

P1DIR = 0x40;
P1OUT = 0;

	while(1){
		P1OUT = 0x40;
		_delay_cycles(50000);
		P1OUT = 0;
		_delay_cycles(50000);
	}

}
